<Window x:Class="PFR_Rendu3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PFR_Rendu3"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Background>
        <ImageBrush/>
    </Window.Background>
    <Grid>
        <Grid.Background>

            <ImageBrush ImageSource="imagefond.jpg"/>
        </Grid.Background>
        <Button x:Name="creer" Content="Creer Personnel" HorizontalAlignment="Left" Height="38" Margin="45,243,0,0" VerticalAlignment="Top" Width="101" Opacity="0.8" Click="creer_Click"/>
        <Button x:Name="attraction" Content="Creer Attraction" HorizontalAlignment="Left" Height="38" Margin="369,243,0,0" VerticalAlignment="Top" Width="101" Opacity="0.8" Click="attraction_Click"/>

    </Grid>
</Window>
